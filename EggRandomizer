--// SERVICES local Players = game:GetService("Players") local Workspace = game:GetService("Workspace") local TweenService = game:GetService("TweenService") local RunService = game:GetService("RunService") local player = Players.LocalPlayer local mouse = player:GetMouse()

--// PET TABLE (Accurate from Grow a Garden) local pets = { ["Common Egg"] = { "Golden Lab", "Dog", "Bunny" }, ["Uncommon Egg"] = { "Black Bunny", "Chicken", "Cat", "Deer" }, ["Rare Egg"] = { "Monkey", "Rooster", "Horse", "Orange Tabby" }, ["Mythical Egg"] = { "Grey Mouse", "Brown Mouse", "Squirrel", "Red Giant Ant", "Red Fox" }, ["Bug Egg"] = { "Snail", "Giant Ant", "Caterpillar", "Praying Mantis", "Dragonfly" }, ["Common Summer Egg"] = { "Starfish", "Crab", "Seagull" }, ["Rare Summer Egg"] = { "Flamingo", "Toucan", "Sea Turtle", "Orangutan", "Seal" }, ["Paradise Egg"] = { "Ostrich", "Peacock", "Capybara", "Scarlet Macaw", "Mimic Octopus" }, ["Dinosaur Egg"] = { "Raptor", "Triceratops", "Stegosaurus", "Pterodactyl", "Brontosaurus", "T‚ÄëRex" }, ["Primal Egg"] = { "Iguanodon", "Parasaurolophus", "Pachycephalosaurus", "Dilophosaurus", "Ankylosaurus", "Spinosaurus" }, ["Oasis Egg"] = { "Ostrich", "Axolotl", "Meerkat", "Sand Snake" }, ["Anti Bee Egg"] = { "Wasp", "Tarantula Hawk", "Moth", "Butterfly", "Disco Bee" }, ["Night Egg"] = { "Hedgehog", "Mole", "Frog", "Echo Frog", "Night Owl", "Raccoon" }, ["Bee Egg"] = { "Bee", "Honey Bee", "Bear Bee", "Petal Bee", "Queen Bee" }, ["Zen Egg"] = { "Shiba Inu", "Nihonzaru", "Tanuki", "Tanchozuru", "Kappa", "Kitsune" } }

--// GUI SETUP local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui")) gui.Name = "EggUtilityGUI"

-- Frame local frame = Instance.new("Frame", gui) frame.Size = UDim2.new(0, 250, 0, 250) frame.Position = UDim2.new(0, 20, 0.3, 0) frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35) frame.BorderSizePixel = 0 frame.Name = "MainFrame"

local uiCorner = Instance.new("UICorner", frame) uiCorner.CornerRadius = UDim.new(0, 12)

-- Title local title = Instance.new("TextLabel", frame) title.Size = UDim2.new(1, 0, 0, 30) title.Text = "üê£ Egg Utility GUI" title.TextColor3 = Color3.new(1, 1, 1) title.BackgroundTransparency = 1 title.Font = Enum.Font.GothamBold title.TextScaled = true

-- Toggle ESP Button local toggleESP = Instance.new("TextButton", frame) toggleESP.Position = UDim2.new(0, 10, 0, 40) toggleESP.Size = UDim2.new(1, -20, 0, 40) toggleESP.Text = "ESP: OFF" toggleESP.BackgroundColor3 = Color3.fromRGB(50, 50, 50) toggleESP.TextColor3 = Color3.new(1, 1, 1) toggleESP.Font = Enum.Font.GothamBold toggleESP.TextScaled = true local espCorner = Instance.new("UICorner", toggleESP) espCorner.CornerRadius = UDim.new(0, 8)

-- Egg Randomizer Button local randomBtn = Instance.new("TextButton", frame) randomBtn.Position = UDim2.new(0, 10, 0, 90) randomBtn.Size = UDim2.new(1, -20, 0, 40) randomBtn.Text = "üé≤ Random Egg" randomBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50) randomBtn.TextColor3 = Color3.new(1, 1, 1) randomBtn.Font = Enum.Font.GothamBold randomBtn.TextScaled = true local randCorner = Instance.new("UICorner", randomBtn) randCorner.CornerRadius = UDim.new(0, 8)

-- Auto Button Placeholder local autoBtn = Instance.new("TextButton", frame) autoBtn.Position = UDim2.new(0, 10, 0, 140) autoBtn.Size = UDim2.new(1, -20, 0, 40) autoBtn.Text = "‚öôÔ∏è Auto: OFF" autoBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50) autoBtn.TextColor3 = Color3.new(1, 1, 1) autoBtn.Font = Enum.Font.GothamBold autoBtn.TextScaled = true local autoCorner = Instance.new("UICorner", autoBtn) autoCorner.CornerRadius = UDim.new(0, 8)

--// ESP LOGIC local espEnabled = false local eggFolder = Workspace:FindFirstChild("Eggs") or Workspace

local function attachESP(egg) if egg:FindFirstChild("EggESP") or not pets[egg.Name] then return end local part = egg:IsA("Model") and egg:FindFirstChildWhichIsA("BasePart") or egg if not part then return end

local bb = Instance.new("BillboardGui", part) bb.Name = "EggESP" bb.Size = UDim2.new(0, 200, 0, 150) bb.StudsOffset = Vector3.new(0, 3, 0) bb.AlwaysOnTop = true

local label = Instance.new("TextLabel", bb) label.Size = UDim2.new(1, 0, 1, 0) label.BackgroundTransparency = 1 label.TextColor3 = Color3.new(1, 1, 0) label.TextStrokeTransparency = 0.6 label.TextScaled = true label.Font = Enum.Font.SourceSansBold label.Text = egg.Name .. "\n" .. table.concat(pets[egg.Name], "\n") end

local function updateESP() for _, egg in ipairs(eggFolder:GetDescendants()) do if pets[egg.Name] then if espEnabled then attachESP(egg) else local part = egg:IsA("Model") and egg:FindFirstChildWhichIsA("BasePart") or egg if part then local bb = part:FindFirstChild("EggESP") if bb then bb:Destroy() end end end end end end

toggleESP.MouseButton1Click:Connect(function() espEnabled = not espEnabled toggleESP.Text = "ESP: " .. (espEnabled and "ON" or "OFF") updateESP() end)

-- Egg Randomizer Action randomBtn.MouseButton1Click:Connect(function() local eggNames = {} for name, _ in pairs(pets) do table.insert(eggNames, name) end local selectedEgg = eggNames[math.random(1, #eggNames)] local petList = pets[selectedEgg] local selectedPet = petList[math.random(1, #petList)] randomBtn.Text = "üé≤ " .. selectedPet .. " from " .. selectedEgg end)

-- Optional: refresh every few seconds RunService.Heartbeat:Connect(function() if espEnabled then updateESP() end end)


