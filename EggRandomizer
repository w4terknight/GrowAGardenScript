--// Services local Players = game:GetService("Players") local Workspace = game:GetService("Workspace") local RunService = game:GetService("RunService") local player = Players.LocalPlayer

--// Eggs & Pets Table local petTable = { ["Common Egg"] = {"Golden Lab", "Dog", "Bunny"}, ["Uncommon Egg"] = {"Black Bunny", "Chicken", "Cat", "Deer"}, ["Rare Egg"] = {"Orange Tabby", "Spotted Deer", "Pig", "Rooster", "Monkey"}, ["Legendary Egg"] = {"Cow", "Silver Monkey", "Sea Otter", "Turtle", "Polar Bear"}, ["Mythical Egg"] = {"Grey Mouse", "Brown Mouse", "Squirrel", "Red Giant Ant", "Red Fox"}, ["Bug Egg"] = {"Snail", "Giant Ant", "Caterpillar", "Praying Mantis", "Dragonfly"}, ["Common Summer Egg"] = {"Starfish", "Seagull", "Crab"}, ["Rare Summer Egg"] = {"Flamingo", "Toucan", "Sea Turtle", "Orangutan", "Seal"}, ["Paradise Egg"] = {"Ostrich", "Peacock", "Capybara", "Scarlet Macaw", "Mini Octopus"}, ["Dinosaur Egg"] = {"Raptor", "Triceratops", "Stegosaurus", "Pterodactyl", "Brontosaurus", "Tâ€‘Rex"}, ["Primal Egg"] = {"Parasaurolophus", "Iguanodon", "Pachycephalosaurus", "Dilophosaurus", "Ankylosaurus", "Spinosaurus"}, ["Oasis Egg"] = {"Ostrich", "Axolotl", "Meerkat", "Sand Snake"}, ["Anti Bee Egg"] = {"Wasp", "Tarantula Hawk", "Moth", "Butterfly", "Disco Bee"}, ["Night Egg"] = {"Hedgehog", "Mole", "Frog", "Echo Frog", "Night Owl", "Raccoon"}, ["Bee Egg"] = {"Bee", "Honey Bee", "Bear Bee", "Petal Bee", "Queen Bee"}, ["Zen Egg"] = {"Shiba Inu", "Nihonzaru", "Tanuki", "Tanchozuru", "Kappa", "Kitsune"} }

local eggNames = {} for eggName, _ in pairs(petTable) do table.insert(eggNames, eggName) end

--// GUI Setup local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui")) screenGui.Name = "PetRandomizerUI"

local mainFrame = Instance.new("Frame", screenGui) mainFrame.Position = UDim2.new(0.02, 0, 0.2, 0) mainFrame.Size = UDim2.new(0, 250, 0, 200) mainFrame.BackgroundColor3 = Color3.fromRGB(66, 44, 25) mainFrame.BorderSizePixel = 0 mainFrame.BackgroundTransparency = 0.05

--// Title local title = Instance.new("TextLabel", mainFrame) title.Text = "Pet Randomizer\nMade by - W4terKnight" title.Size = UDim2.new(1, 0, 0, 40) title.TextColor3 = Color3.new(1,1,1) title.BackgroundTransparency = 1 title.TextScaled = true title.Font = Enum.Font.SourceSansBold

--// Egg Input TextBox local eggInput = Instance.new("TextBox", mainFrame) eggInput.PlaceholderText = "Type egg name (e.g. Bug Egg)" eggInput.Size = UDim2.new(1, -10, 0, 30) eggInput.Position = UDim2.new(0, 5, 0, 40) eggInput.BackgroundColor3 = Color3.fromRGB(80, 60, 40) eggInput.TextColor3 = Color3.new(1, 1, 1) eggInput.TextScaled = true eggInput.Font = Enum.Font.SourceSans

--// Create Button Helper local function createButton(name, text, color, posY) local button = Instance.new("TextButton", mainFrame) button.Name = name button.Text = text button.Size = UDim2.new(1, -10, 0, 35) button.Position = UDim2.new(0, 5, 0, posY) button.BackgroundColor3 = color button.TextScaled = true button.TextColor3 = Color3.new(1, 1, 1) button.Font = Enum.Font.SourceSansBold return button end

local randomizeBtn = createButton("RandomizeBtn", "Randomize Pets", Color3.fromRGB(255, 140, 0), 75) local espBtn = createButton("ESPBtn", "ESP: OFF", Color3.fromRGB(40, 40, 40), 115) local autoBtn = createButton("AutoBtn", "Auto: OFF", Color3.fromRGB(0, 200, 0), 155)

--// Pet Randomizer local function randomizePet() local inputEgg = eggInput.Text if petTable[inputEgg] then local petList = petTable[inputEgg] local chosenPet = petList[math.random(1, #petList)] warn("[Randomizer] Hatched from " .. inputEgg .. ": " .. chosenPet) else warn("[Randomizer] Invalid egg name: " .. inputEgg) end end

--// ESP Function local espEnabled = false local function toggleESP() espEnabled = not espEnabled espBtn.Text = "ESP: " .. (espEnabled and "ON" or "OFF")

for _, egg in ipairs(Workspace:GetDescendants()) do
	if egg:IsA("Part") then
		for eggType, pets in pairs(petTable) do
			if string.find(egg.Name, eggType) then
				if espEnabled then
					if not egg:FindFirstChild("EggESP") then
						local billboard = Instance.new("BillboardGui", egg)
						billboard.Name = "EggESP"
						billboard.Size = UDim2.new(0, 150, 0, 50)
						billboard.StudsOffset = Vector3.new(0, 3, 0)
						billboard.AlwaysOnTop = true

						local label = Instance.new("TextLabel", billboard)
						label.Size = UDim2.new(1, 0, 1, 0)
						label.BackgroundTransparency = 1
						label.TextColor3 = Color3.fromRGB(255, 255, 0)
						label.TextScaled = true
						label.Font = Enum.Font.GothamBold

						local petsText = table.concat(pets, ", ")
						label.Text = eggType .. "\n[" .. petsText .. "]"
					end
				else
					if egg:FindFirstChild("EggESP") then
						egg:FindFirstChild("EggESP"):Destroy()
					end
				end
			end
		end
	end
end

end

--// Auto Hatch Functionality local autoEnabled = false local function toggleAuto() autoEnabled = not autoEnabled autoBtn.Text = "Auto: " .. (autoEnabled and "ON" or "OFF") end

RunService.RenderStepped:Connect(function() if autoEnabled then randomizePet() wait(1.5) end end)

--// Button Click Events randomizeBtn.MouseButton1Click:Connect(randomizePet) espBtn.MouseButton1Click:Connect(toggleESP) autoBtn.MouseButton1Click:Connect(toggleAuto)

